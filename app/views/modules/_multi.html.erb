<% modules = submodules %>
<div class="module module-col-<%= cols %> module-multi" data-uuid="<%= uuid %>">
  <% modules.each do |m| %>
    <%= render 'modules/submodule', m: m, multi: true %>
  <% end %>
  <% if @homepage_editing %>
    <%= render 'modules/new_submodule_toolbar', module_uuid: uuid %>
  <% end %>
</div>